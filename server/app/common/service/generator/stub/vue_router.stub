import { RouteRecordRaw, RouterView } from 'vue-router'

const routes: RouteRecordRaw = {
    path: '/{DIR_NAME}',
    redirect: '/{DIR_NAME}/index',
    component: RouterView,
    meta: { title: '{TITLE}', icon: 'icon-setting' },
    children: [
        {
            path: '/{DIR_NAME}/index',
            component: () => import('@/views/{DIR_NAME}/index.vue'),
            meta: { title: '{TITLE}' },
        },
    ],
}

export default routes
