<el-form-item label="{COLUMN_COMMENT}" prop="{COLUMN_NAME}">
    <data-picker
        v-model:start_time="formData.start_{COLUMN_NAME}"
        v-model:end_time="formData.end_{COLUMN_NAME}"
    />
</el-form-item>

<el-form-item label="{COLUMN_COMMENT}" prop="{COLUMN_NAME}">
    <el-date-picker 
        v-model="formData.{COLUMN_NAME}"
        clearable
        type="date"
        value-format="YYYY-MM-DD HH:mm:ss"
        placeholder="选择{COLUMN_COMMENT}">
    </el-date-picker>
</el-form-item>
